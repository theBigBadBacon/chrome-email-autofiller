{
    "name": "Form filler 5000 XX eXtreme",
    "version": "1.2",
    "description": "Generates unique email addresses that auto-fills email fields. Optional - configure your own email and other form fields.",
    "permissions": ["storage", "activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "manifest_version": 2
  }